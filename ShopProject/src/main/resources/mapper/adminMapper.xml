<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.scmaster.shopping.dao.ShopAdminMapper">
	<select id="shop_admin_select" parameterType="string" resultType="Admin">
		select
		    admin_id
		    ,admin_pw
		from
		    shop_admin_tb
		where
		    admin_id = #{admin_id}
	</select>
	
	
	
	
	
	
</mapper>
